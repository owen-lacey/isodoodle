{
  "name": "isodoodle",
  "version": "0.1.1",
  "description": "A minimal TypeScript library for drawing SVGs on an isometric grid",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "svg",
    "isometric",
    "drawing",
    "graphics",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owen-lacey/isodoodle.git"
  },
  "homepage": "https://isodoodle.netlify.app",
  "bugs": {
    "url": "https://github.com/owen-lacey/isodoodle/issues"
  },
  "author": "owen-lacey",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
